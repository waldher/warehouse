<%= link_to "<< Back", admin_index_url %>
<table class="dataTable" style="width: 100%">
  <thead>
    <tr>
      <th>Email</th>
      <th>Key</th>
      <th>link</th>
    </tr>
  </thead>
  <tbody>
  <% @realtors.each do |realtor| %>
    <tr>
      <td><%= link_to realtor.email_address, edit_customer_url(realtor) %></td>
      <td><%= realtor.key %></td>
      <td><%= raw(auto_link("http://#{request.host_with_port}/first_login/#{realtor.key}")) if realtor.key.present? %></td>
    </tr>
  <% end %>
  </tbody>
</table>
